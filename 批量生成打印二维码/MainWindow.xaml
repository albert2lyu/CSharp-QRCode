<Window x:Class="批量生成打印二维码.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:批量生成打印二维码"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <TextBox Name="textBox1" Height="20" Width="100" Margin="10 10 2 10">1</TextBox>
            <TextBlock Height="15">张</TextBlock>
            <Button Click="Button_Click" Margin="10" Width="100" Height="30">Go</Button>
        </StackPanel>
        
        <Border Grid.Row="1" BorderThickness="1" BorderBrush="Gray">
            <Canvas Name="canvas1"></Canvas>
        </Border>
        
    </Grid>
</Window>
